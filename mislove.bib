@inproceedings{Tucker07:OPIUM,
	Address = {Minneapolis, MN},
	Author = {Chris Tucker and David Shuffelton and Ranjit Jhala and Sorin Lerner},
	Booktitle = {International Conference on Software Engineering (ICSE)},
	Contribution1 = {The authors formulate package-installability as a SAT problem, which guarantees completeness: the SAT solver will always find a solution where one exists, and it will indicate the critical conflicts when a package is not installable, which are used to determine what packages have to be uninstalled. The optimization of an arbitrary objective function is formulated as a pseudo-boolean minimization constraint (by attaching weights to the boolean variables that correspond to each package), which can be translated into an Integer Linear Programming (ILP) problem (the results indicate that the pseudo-boolean solver is faster than the ILP solver).},
	Contribution2 = {In addition to solving the install problem, Opium leverages its optimization functionality to solve the uninstall problem while removing a minimal number of packages.},
	Contribution3 = {Opium's performance is reasonable. While the solver is 3.5 -- 6 times slower than APT, this can probably be optimized since half of this time is spent reading the distribution. Moreover, the solver runtime (20 s on average, when there are conflicts) is amortized by the time needed to download the packages (especially since Opium tries to optimize the number of bytes downloaded).},
	Date-Added = {2014-09-13 23:04:09 +0000},
	Date-Modified = {2014-09-13 23:04:09 +0000},
	Doi = {http://doi.ieeecomputersociety.org/10.1109/ICSE.2007.59},
	Interesting = {high},
	Month = {May},
	Pages = {178--188},
	Summary = {Because of the complexity of dependencies among packages from Linux distributions, current package managers are either incomplete -- they fail to find a solution for installable packages -- ({\em e.g.}, YUM~\cite{Brown:YUM}, APT~\cite{Silva:APTHowto}, which use heuristic algorithms) or inefficient ({\em e.g.}, Smart~\cite{Niemeyer:Smart}, which performs an exhaustive search). Opium always finds a solution where one exists and it can optimize a user-provided objective function that defines the best solution. The authors compare Opium with APT on 600 real-world traces; Opium is up to 6 times slower, while APT fails to find a solution for 0.61\% of installable packages (which affects 23.3\% of users).},
	Title = {{OPIUM}: Optimal Package Install/Uninstall Manager},
	Weakness1 = {Since a SAT solver is used to determine what packages to install and uninstall, this raises the question whether the problem solved is NP-complete or not and whether a better algorithm may exist. While the SAT solver is guaranteed to find a solution where one exists, it is not guaranteed to terminate fast (for certain corner cases, its runtime may be exponential in the size of the repository). APT~\cite{Silva:APTHowto}) restricts the number of backtracks performed due to conflicts in order ensure fast-termination.},
	Weakness2 = {In a real user trace, APT uninstalled 61 packages, including the Linux kernel, while Opium removed 22 packages, none of which were the kernel. The authors state that ``the poor user would not be able to reboot the machine'' after this installation, but it is not clear if APT really corrupted the machine (by not installing a new kernel instead) and how often such corruptions occur.},
	Year = {2007},
	Bdsk-Url-1 = {http://doi.ieeecomputersociety.org/10.1109/ICSE.2007.59}}

