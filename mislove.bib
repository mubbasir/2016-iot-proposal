@inproceedings{ajmani06:distributed,
	Abstract = {Upgrading the software of long-lived,
                  highly-available distributed systems is
                  difficult. It is not possible to upgrade all the
                  nodes in a system at once, since some nodes may be
                  unavailable and halting the system for an upgrade is
                  unacceptable. Instead, upgrades must happen
                  gradually, and there may be long periods of time
                  when different nodes run different software versions
                  and need to communicate using incompatible
                  protocols. We present a methodology and
                  infrastructure that make it possible to upgrade
                  distributed systems automatically while limiting
                  service disruption. We introduce new ways to reason
                  about correctness in a multi-version system. We also
                  describe a prototype implementation that supports
                  automatic upgrades with modest overhead.},
	Address = {Nantes, France},
	Author = {Sameer Ajmani and Barbara Liskov and Liuba Shrira},
	Booktitle = {European Conference on Object-Oriented Programming},
	Contribution1 = {This technique is very generic: it allows objects to be replaced with versions containing arbitrary modifications; versions can be incompatible as long as the programmers can define invariants between the states of the two objects. Some methods may have to be disallowed for this purpose, but they will eventually be enabled. Temporarily disallowing APIs makes sense in RPC-based distributed systems because communication errors may happen anytime and programs are written to handle this situation.},
	Contribution2 = {Other advantages of this approach: it allows the distributed system to provide continuous service (even though individual nodes are unavailable while they upgrade), upgrades are defined at a central location and they propagate through the system, objects are upgrading according to a schedule that may take into account the impact on performance (distributed ``scheduling functions'' are attached to each upgrade).},
	Contribution3 = {Presents a formal specification of a distributed system with multiple co-existing versions of objects (``mixed mode''), defining conditions for state consistency, for disallowing calls (all the methods of the current object should be available) and describing invariants that tie two versions of an object together.},
	Contribution4 = {Past versions can be ``retired'' when they are not needed anymore.},
	Date-Added = {2014-09-13 23:06:50 +0000},
	Date-Modified = {2014-09-13 23:06:50 +0000},
	Interesting = {high},
	Month = {Jul},
	Opinions = {Good work, but only solves half of the problem: for such an approach to be useful in a real system, a solution for the dependency-tracking issue would be needed.},
	Opturl = {http://pmg.csail.mit.edu/upgrades/ajmani06modular-abstract.html},
	Pages = {452--476},
	Summary = {Presents Upstart, a prototype for gradually upgrading distributed systems (one node at a time, restarting the node after the upgrade), which is based on multiple co-existing versions of an object, each holding its own state and running in lock-step (with some incompatible methods disabled). Upstart makes the distinction between the current object and ``simulation objects'' -- implementing the APIs of past and future versions -- and is implemented using library interposition, with simulation objects intercepting invocations and delegating them to the other versions.},
	Title = {Modular Software Upgrades for Distributed Systems},
	Weakness1 = {No mention of dependency tracking; for example, when are versions (``simulation objects'') retired, how does the upgrade layer know which local object handles a particular version of another object, how are upgrades done for nested objects.},
	Weakness2 = {The authors claim that only in some ``pathological cases'' an object needs to keep more than one past simulation object, but they don't explain what these pathological conditions are. It seems that for multiple incompatible upgrades (different APIs), the objects would need to keep an unbounded number of past simulation objects.},
	Weakness3 = {The authors state that the previous approaches based on wrappers offer less flexibility, but they do not compare their approach against Segal\&Frieder.},
	Weakness4 = {Coarse-grained upgrades: the root object of each node is replaced, along with all the other objects it references locally.},
	Weakness5 = {The experimental section is inconclusive because of significantly restricted benchmarks. Maximum latencies are not reported. The authors claim that in a large system the clients are not likely to access a node that is upgrading.},
	Year = {2006}}

@article{segal93:on_the_fly,
	Abstract = {Several dynamic software-based updating systems that
                  are in the research and production stages are
                  described. In particular, the procedure-oriented
                  dynamic updating system (PODUS) is discussed. In
                  PODUS, a program is updated by loading the new
                  version of the program and replacing each old
                  procedure with its corresponding new procedure
                  during execution. Updating a procedure involves
                  changing the binding from its current version to the
                  new version. When all procedures have been replaced
                  by their corresponding new versions, the program
                  update is completed.},
	Author = {Mark Segal and Ophir Frieder},
	Contribution1 = {The authors identify a set of general requirements for any dynamic updating system: preserving program correctness (during and after the upgrade), minimizing human intervention (both during and after the upgrade), supporting program restructuring and low-level program changes (e.g., both implementations and interfaces), supporting distributed programs (communicating across mutually distrustful administrative domains), not requiring special-purpose hardware and not constraining the language and environment. They also indicate that all updating systems require inserting a layer of indirection between the program modules that invoke each other.},
	Contribution2 = {Replacing abstract data types or modules: an old implementation is changed with a new one; version tagging and data conversion may also be required. This does not address the general problem of code restructuring.},
	Contribution3 = {Replacing servers in client-server systems: similar, but coarser granularity than previous approach. May need to update all dependent services simultaneously; the services are disabled during the update; can use crash-recovery facilities for providing data consistency (e.g., Bloom & Liskov's Argus). Assumes that only the implementation of a service changes between versions, but not the interface.},
	Contribution4 = {Updating in constrained message-passing systems (e.g., Kramer & Magee's Conic): modules communicate unidirectional, typed channels; a configuration manager handles updating by changing the module connections and copying state when modules are inactive. This precludes a module from selecting where to send a request at runtime.},
	Contribution5 = {Updating programs in procedural languages (e.g., C): more generic (can do general code restructuring, procedural languages are everywhere), harder to do (need to handle all the hairy details of a language like C). May require changes to the compiler/OS for building a transparent indirection layer; may require access to source code or other compilation artifacts (e.g., the procedure-call graph).},
	Contribution6 = {States that dynamic linking does not provide true dynamic updating capabilities because changing a dynamic library typically requires restarting the program, there is no correctness-preserving mechanism and there are no provisions for mapping library state across versions.},
	Contribution7 = {Presents PODUS (designed by the authors): changes virtual memory system and C compiler to handle procedure addresses that contain location, version information and that use a binding table as layer of indirection. PODUS loads a new version of a program into memory and then upgrades each procedure by changing its address in the binding table. Only inactive procedures are updated (not on the program stack and cannot invoke another active procedure, either directly or indirectly); static data is copied over. All inactive procedures are updated before one of them is invoked. ``Interprocedures'' are used to allow an old procedure to invoke a new one (with a possibly modified interface); they perform all the necessary data conversions and they must be provided together with the new version of the program. The upgrade process is coordinated (possibly remotely) by an ``updating system shell'. By using RPC to preserve procedure-call semantics across procedure boundaries, PODUS can handle distributed systems with only few modifications.},
	Contribution8 = {The requirements of PODUS: programs must be written in a top-down manner (only inactive procedures are updated and the main() function is never inactive); data used by global procedures (e.g., global variables) is accessed through abstract data types. Requires programmers to specify all the possible hidden dependencies between procedures. Programs must have a single thread of control. Backward compatibility in PODUS: an old procedure can invoke a new one (through and interprocedure), but not the other way. PODUS can upgrade distributed C programs in a pretty general way, transparently to the application programmer (the OS and compiler require some changes), preserving correctness and with little overhead (the upgrade requires only modifying the addresses in the binding table; the indirection can be efficiently implemented through the virtual memory system. PODUS can change both the implementation and the interface of a procedure, as long as the backward compatibility requirement is satisfied (the sequence of procedures invoked in the main() routine must stay the same).},
	Contribution9 = {Identifies the problem of hidden dependencies (``semantic dependencies''): procedures can communicate through side-channels (e.g., thorough the physical environment) and these dependencies cannot be detected from the program's syntax.},
	Date-Added = {2014-09-13 23:06:50 +0000},
	Date-Modified = {2014-09-13 23:06:50 +0000},
	Interesting = {high},
	Journal = {IEEE Software},
	Month = {Mar},
	Number = 2,
	Opinions = {There is more recent work that uses similar approaches as PODUS: Neamtiu and Hicks' Dynamic SW Upgrading; the Erlang functional language developed by Ericsson; Boyapati & Liskov, OOPSLA 03. While dynamic linking is not an updating system in itself, it can be used to build one (e.g., many modern plugin architectures).},
	Opturl = {http://ieeexplore.ieee.org/search/wrapper.jsp?arnumber=199735},
	Pages = {53--65},
	Summary = {Presents the design of the PODUS dynamic updating system, as well as a thorough qualitative comparison with other systems for dynamic updating.},
	Title = {On-the-fly program modification: Systems for dynamic updating},
	Volume = 10,
	Weakness1 = {What happens if the updating system shell crashed in the middle of an upgrade?},
	Weakness2 = {All the inactive procedures (potentially distributed on many computers) must be upgraded together before one of them can be invoked: how is this enforced? What happens if they are invoked before the upgrade is complete?},
	Weakness3 = {The authors do not state clearly all the requirements of C programs that can be updated using PODUS; for instance, I don't see how they could handle longjmp().},
	Weakness4 = {What is the reason for the backward compatibility requirement, stating that a new procedure may not invoke an old one (this is the reason for updating all the inactive procedures together and for the dependency analysis based on the procedure-call graph constructed at compile-time)?},
	Year = 1993}

@article{KramerMagee90:evolving_philosophers,
	Abstract = {A model for dynamic change management which
                  separates structural concerns from component
                  application concerns is presented. This separation
                  of concerns permits the formulation of general
                  structural rules for change at the configuration
                  level without the need to consider application
                  state, and the specification of application
                  component actions without prior knowledge of the
                  actual structural changes which may be
                  introduced. In addition, the changes can be applied
                  in such a way so as to leave the modified system in
                  a consistent state, and cause no disturbance to the
                  unaffected part of the operational system. The model
                  is applied to an example problem, 'evolving
                  philosophers'. The principles of this model have
                  been implemented and tested in the Conic environment
                  for distributed systems.},
	Author = {Jeff Kramer and Jeff Magee},
	Contribution1 = {Identify several drivers for change: the evolution of human needs, of technology and of the application environment.},
	Contribution2 = {Requirements for applying this approach: detailed software architecture (components, connectors), minimal change-management API provided by all the components (passivate, assert(active/passive), activate, link, unlink).},
	Contribution3 = {The workflow of the application is composed of transactions: communication between two nodes, where the initiator is informed of the completion of the transaction. This change management framework is mostly concerned with independent transactions, whose completion does not depend on other, possibly nested, transactions.},
	Contribution4 = {{\em Quiescence} (no communication in progress) is needed for keeping the system in a consistent state during and after the change.},
	Contribution5 = {The control API enforces a {\em passive} state, where a node does not initiate new transactions, but continues to accept and service transaction requests. If all the inbound nodes of a node $Q$ are passive, then $Q$ is quiescent.},
	Contribution6 = {The authors explain how to perform change management on a distributed ``dining philosophers'' protocol. Changes include: creating a philosopher community, creating/removing an individual philosopher and merging two philosopher rings.},
	Contribution7 = {There are two approaches for dealing with dependent (e.g., nested) transactions: (i) the application must know all the transaction dependencies, for nodes to decide if a transaction has completed (in this case, a passive node must initiate consequent transactions and the set of nodes that need to be passivated has to be extended to include all indirect dependencies); (ii) the transactions with consequents can be aborted by a passive node and recovery must be performed (i.e., the transactions are atomic).},
	Date-Added = {2014-09-13 23:06:50 +0000},
	Date-Modified = {2014-09-13 23:06:50 +0000},
	Doi = {http://dx.doi.org/10.1109/32.60317},
	Interesting = {high},
	Journal = {{IEEE} Transactions on Software Engineering},
	Number = {11},
	Opinions = {This approach relies on support from the application (in terms of a change-management API), rather than on modifying the OS/compiler/linker to insert the appropriate hooks. Its most important contribution is in presenting the minimal API that a component must provide for change management. Such applications need to have provisions for structural change-management from the development phase.},
	Pages = {1293--1306},
	Summary = {The authors argue that changes should be specified as structural change (such as component connection/disconnection) and should be separate from functional concerns (such as restoring the correct state). Each component must provide a control API for preparing and executing the change, and should be prepared to handle state transfers if necessary. The change management framework can derive the correct sequence of control API calls from a high-level, declarative change specification. This approach does not require recovery. The authors have implemented this approach in the Conic environment for distributed systems.},
	Title = {The Evolving Philosophers Problem: Dynamic Change Management},
	Volume = {16},
	Weakness1 = {What kind of real-world application would be able to use this technique? What are the implications of passivating a node?},
	Weakness2 = {What are the technical means for unlinking dangling connections?},
	Weakness3 = {How do the components handle state transfers? (the paper states that ``state is passed to the environment'').},
	Weakness4 = {From the ``evolving philosophers'' example, it seems that passivate requests can be denied by a component. It is unclear how the change management handles denied control requests.},
	Weakness5 = {What happens when a transaction is not accepted by a node?},
	Weakness6 = {To replace a server, you have to passivate all the clients. In a 3-tier Web system (where most of the transactions are dependent) upgrading the database would probably require passivating the whole system.},
	Weakness7 = {The authors argue that the Frieder&Segal approach is focused on replacement, while their technique works for arbitrary restructuring. However, it seems that procedure replacements could be used for arbitrary restructuring, including permanent additions/removals of procedures.},
	Year = {1990},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/32.60317}}

