@unpublished{Russel03:RPM_hell,
	Author = {Sean Russel},
	Date-Added = {2014-09-13 23:00:20 +0000},
	Date-Modified = {2014-09-13 23:00:20 +0000},
	Note = {Online article},
	Summary = {This is a self-published online rant, but a very informative one. It states that many configuration problems stem from the fact that modern operating systems have shared libraries, which may be changed or deleted by package management software such as RPM, which may break some of the existing software. It also identifies some problems that are specific to RPM: you cannot define optional dependencies (e.g., don't install the GUI, which depends on X), which sometimes forces to applications to be split across multiple RPM packages; has only 1-level dependency resolution and doesn't know how to resolve a dependency tree; has a single-version dependency mechanism (you cannot specify a range of versions that would work), which exacerbates the shared library problem; RPM packages are monolithic (specification + actual software), which means that it is impossible to make complex queries on the structure of dependencies unless you download the entire repository; RPM makes it hard to install multiple versions of the same package on a system; software installed through other mechanisms cannot be detected (if a dependency is not in the database, it doesn't exist for RPM). The article doesn't talk about tools that were built on top of RPM to alleviate some of these problems (e.g., yum, apt-rpm), but it does review a different package management system (Portage) that addresses some of the mentioned shortcomings (more flexible dependency specifications, can resolve a dependency tree, limited support for co-existing multiple versions, source-based, which means that software is built and optimized on the system of destination).},
	Title = {RPM Hell},
	Url = {http://www.germane-software.com/~ser/Files/Essays/RPM_Hell.html},
	Bdsk-Url-1 = {http://www.germane-software.com/~ser/Files/Essays/RPM_Hell.html}}

@inproceedings{Dig06:automated_detection_refactorings,
	Address = {Nantes, France},
	Author = {Danny Dig and C. Comertoglu and D. Marinov and R. Johnson},
	Booktitle = {European Conference on Object-Oriented Programming},
	Date-Added = {2014-09-13 23:00:20 +0000},
	Date-Modified = {2014-09-13 23:00:20 +0000},
	Month = {Jul},
	Pages = {404--428},
	Summary = {Follow-up work to the authors' previous work on API evolution\cite{Dig06:API_evolution}. We know that refactorings are important; there are tools that partially detect refactorings; there are tools that can record and replay refactorings (CatchUp, JBuilder, planned for Eclipse 3.2). This paper tries to put all this together to enable the automatic update of clients using framework subject to API evolution. The idea is to detect refactorings in the framework automatically and to replay them in the client. The detection algorithm combines syntactic and semantic analysis in the following way: parses the two versions of the source files into abstract syntactic trees, then computes digital fingerprints (shingles) of methods, classes and packages to find similar pairs of entities, then performs semantic analysis (call graph, class hierarchy) on these similar entities. This tool outputs refactoring logs that can later be replayed using some other existing tools. The authors test their approach on codebases up to 350 KLOC, with a resulting accuracy of at least 85\%.},
	Title = {Automated Detection of Refactorings in Evolving Components},
	Url = {http://netfiles.uiuc.edu/dig/papers/DetectRefactoringsECOOP.pdf},
	Year = {2006},
	Bdsk-Url-1 = {http://netfiles.uiuc.edu/dig/papers/DetectRefactoringsECOOP.pdf}}

@article{Dig06:API_evolution,
	Author = {Danny Dig and Ralph Johnson},
	Date-Added = {2014-09-13 23:00:20 +0000},
	Date-Modified = {2014-09-13 23:00:20 +0000},
	Journal = {Journal of Software Maintenance and Evolution (JSME)},
	Month = {Mar/Apr},
	Number = {2},
	Opturl = {http://netfiles.uiuc.edu/dig/papers/JSME_API_Evolution.pdf},
	Pages = {83--107},
	Summary = {Examines how APIs evolve for five medium-sized to large Java frameworks and libraries. This was done by manual analysis of software artifacts (release notes, source code), cross-checked with the output of a tool (Van) for automatic detection of API changes. Changes can be either non-functional (transparent bug-fixes or performance enhancements) or functional. The functional changes are either non-breaking (e.g., new APIs, deprecated methods, on-demand loading of legacy libraries) or breaking changes (the program is backwards-incompatible). The breaking API changes are either refactorings (modifications of the program structure) or intended behavioral changes, which modify the semantics of the application. Examples of refactorings are (in the order of the observed frequencies): moved method, moved field, deleted method, changed argument type, etc. Examples of behavioral changes are: new method contract (weaker/stronger pre/post conditions), implement new interface, changed event order (events fired by the framework), and new enum constant (may break some switch/case statements that rely on the number of constants). A breaking refactoring may result either in a compilation error, or an unintended behavioral change. The authors conclude that, while refactorings represent only a small part of all the API changes (less than 30\%), they have a big impact because between 81\% - 100\% of the breaking changes are refactorings.},
	Title = {{How do APIs evolve? A story of refactoring}},
	Url = {http://dx.doi.org/10.1002/smr.328},
	Volume = {18},
	Weakness1 = {It remains unclear whether the provided list of breaking behavioral changes is exhaustive and the authors provide no assurance that, through inspection, they were able to detect all these changes.},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/smr.328}}

