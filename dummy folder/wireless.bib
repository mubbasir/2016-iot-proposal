@inproceedings{kawadia:ewind2005,
  author    = {Vikas Kawadia and {P.R.} Kumar},
  title     = {Experimental Investigations into {TCP} performance over wireless multihop networks.},
  booktitle = {{SIGCOMM} E-WIND Workshop},
  year      = {2005},
  annote = " This paper presents an experimental study of the performance of TCP over multihop wireless networks. An indoor 802.11b
  testbed is used. The three main conclusions are: (i) limiting the maximum window size improves end-to-end delay and delay jitter and the optimal
  maximum window size is $\frac{3}{2}n$ where $n$ is the number of hops. (ii) turning off RTS/CTS improves throughput,
   delay and delay jitter. (iii) TCP-SACK option does not have any statistically significant effect on TCP performance. In our experiments we plan to
   verify these results."
}

@inproceedings{kuang:spects2003,
  author    = {Tianbo Kuang and Fang Xiao and Carey Williamson},
  title     = {Diagnosing wireless {TCP} performance problems: A case study.},
  booktitle = {Proc. of SPECTS},
  year      = {2003},
  annote = " This paper studies the performance of TCP in a wireless LAN testbed. The results show that the USB bus of the wireless cards,
  the TCP implementation and MAC the protocol can all affect the overall TCP performance. Although we are interested in multihop scenarios,
  this paper gives us some guidelines on how to conduct our experiments. Also, we will try to verify the results related to MAC layer interactions
   in our scenarios."
}


@Book{anastasi:book_chapter2007,
   author = {Giuseppe Anastasi and Emilio Ancilloti and Marco Conti and Andrea Passarella},
   title = {Experimental analysis of {TCP} performance in static multi-hop Ad hoc networks.},
   book  = {{Marco Conti and Jon Crowcroft and Andrea Passarella}, editors, Mobile Ad Hoc Networks: from Theory to Reality , Chapter 6},
   publisher = {Nova Science Publisher},
   year = {2007},
   annote = " This book chapter studies the impact of the maximum congestion window size on TCP performance on a static wireless testbed.
   Differently from the other related work, it also takes into account the effect of the routing protocol, using two different routing protocols,
   AODV and OLSR. It finds that in a chain topology with four or less hops, a maximum window size equal to 3 achieves optimal performance. We will use
   similar methodology for our measurements, but we will not use any routing protocol, because we do not want control routing packets to interfere
   with our generated traffic."
}

@inproceedings{gupta:mascots2004,
  author    = {Abhinav Gupta and Wormsbecker and Carey Williamson},
  title     = {Experimental Evaluation of {TCP} performance in multi-hop wireless ad hoc networks.},
  booktitle = {Proc. of MASCOTS},
  year      = {2004},
  annote = " This is another experimental analysis on TCP performance. As opposed to the previous papers, this work considers both static and
  mobile clients. They verify that performance decreases with the number of hops and with the speed of the mobile client. They also evaluate the
  impact of Rate Based Pacing (RBP) on TCP performance. Surprisingly, results show that TCP Reno outperforms TCP with RBP. We will use similar the same
  tools for our measurement study, but we will only consider static scenarios."
}

@inproceedings{oliveira:infocom2005,
  author    = {Ruy de Oliveira and Torsten Braun},
  title     = {A dynamic adaptive acknowledgement strategy for {TCP} over multihop wireless networks.},
  booktitle = {{IEEE} Infocom},
  year      = {2005},
  annote = " This paper identifies contention between data and acknowledgement packets and spurious retransmissions as two of the main factors
  that affect TCP performance in multihop wireless networks. Consequently, it proposes a dynamic adaptive strategy for minimizing the number of ACK
  packets in transit and mitigating spurious retransmissions. In contrast to the previous papers, it proposes a maximum window size equal to 4,
  as the optimal value. We will investigate the value for the optimal window size in our testbed."
}

@inproceedings{chen:icc2003,
  author    = {Kai Chen and Yuan Xue and Klara Nahrstedt},
  title     = {On setting {TCP}'s congestion window limit in mobile ad hoc networks.},
  booktitle = {{IEEE} International Conference on Communications ({ICC}) },
  year      = {2003},
  annote = " This paper studies how to properly set TCP's optimal congestion window limit in order to achieve optimal performance in multihop
  wireless networks. Differently from the other papers, it considers both the forward and the reverse path in calculating the optimal value. Using
  analysis and simulations they show that this value equals 1/5 of the round-trip hop count of the path. We will verify this result in our testbed."
}

@inproceedings{fu:infocom2003,
  author    = {Zhenghua Fu and Petros Zerfos and Haiyun Luo and Songwu Lu and Lixia Zhang and Mario Gerla},
  title     = {The impact of multihop wireless channel on {TCP} throughput and loss.},
  booktitle = {{IEEE} Infocom},
  year      = {2003},
  annote = " This paper studies the impact of multihop wireless channel on TCP's performance. It is the first to show that for a given topology and traffic
  pattern, there exists an optimal value of the TCP congestion window size. For a chain topology, they calculate this value equal to 1/4 of the chain
  length, taking into account interference range and spatial reuse. They also find that TCP typically exceeds this optimal window, and this leads to
  decreased throughput and increased loss. Similar to the other papers that propose an optimal value for the congestion window size, we plan to study
  if this value is valid in our testbed."
}

@inproceedings{gerla:icc1999,
  author    = {Mario Gerla and Rajive Bagrodia and Lixia Zhang and Ken Tang and Lan Wang},
  title     = {{TCP} over wireless multi-hop protocols: simulations and experiments.},
  booktitle = {{IEEE} International Conference on Communications ({ICC})},
  year      = {1999},
  annote = " This paper identifies two problems of TCP over wireless multihop networks. The conflict between data packets and ACKs, which causes TCP
  performance degradation for window sizes larger than one, and the interaction between MAC and TCP layer backoff timers, which causes unfairness
  and capture conditions. The paper considers MAC layer protocols older than 802.11 and concludes that MAC layer acknowledgements are necessary to
  reduce link loss rates. This feature exists in 802.11, hence the significance of this paper is now limited."
}

@inproceedings{bakshi:icdcs1997,
  author    = {Bikram S. Bakshi and P. Krishna and Nitin H. Vaidya and Dhiraj K. Pradhan},
  title     = {Improving Performance of {TCP} over Wireless Networks.},
  booktitle = {{IEEE} International Conference on Distributed Computing Systems ({ICDCS})},
  year      = {1997},
  annote = " This paper studies TCP performance in a wireless LAN. It considers a simple topology consisted of a base station, a wired host and
  a wireless mobile host. It examines the effect of burst errors on the wireless link, packet size variation on the wired network, local error recovery
  by the base station, and explicit feedback by the base station. The scenario under consideration is different than our target scenarios."
}

