@article{xu:wcmc2001,
 author = {Shugong Xu and Tarek Saadawi},
 title = {Performance evaluation of {TCP} algorithms in multi-hop wireless packet networks.},
 journal = {Wireless Communications and Mobile Computing},
 volume = {2},
 number = {1},
 year = {2001},
 annote = "This paper studies the performance of different TCP versions (Tahoe, Reno, New Reno, Sack and Vegas) in an 802.11 LAN through simulations.
 Interestingly, they find that TCP Vegas performs best. For the other versions, they find that performance can be improved by limiting the maximum
 window size to 4. We will verify this in our testbed. They also show that all versions re benefitted by the use of delayed ACKs. We are currently not
 sure whether Linux TCP implementation uses delayed ACKs, but we will investigate it if time permits."
 }


